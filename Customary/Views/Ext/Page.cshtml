@model Custom.Models.ViewModel
@{
    ViewBag.Title = Model.PageTitle;
    Layout = Model.Layout ?? "~/Views/App/_Viewport.cshtml";
}
@section styles
{
}
@section scripts
{
    @Html.Partial(Model.ViewName, Model)
    <script>
        Ext.onReady(function () {
            var view = Ext.create('@Model.ClassName', {});
            window.Viewport = Ext.create('Ext.container.Viewport', {
                items: [
                    view
                ]
            });
        });
    </script>
}